<h2>Edit <%= @gym_session.name %></h2>
<form method="POST" action="/gym_sessions/<%= @gym_session.id %>">
  <div class="form-wrapper">

    <div class="form-el">
      <label for="name">Name:</label>
      <input type="text" name="name" id="name" value="<%= @gym_session.name %>"/>
    </div>

    <div class="form-el">
      <label for="type">Type:</label>
      <input type="text" name="type" id="type" value="<%= @gym_session.type %>"/>
    </div>

    <div class="form-el">
      <input type="submit" value="Edit Gym Session"/>
    </div>
