<h2>Add new booking</h2>
<form method="post"action="/bookings">

  <div class="gym_member"
  <label for="gym_member">Gym_member:</label>
  </div>
  <select name="member_id" id="gym_member">
    <% Member.all.each do |member| %>
    <option value="<%= member.id %>"><%= member.name %></option>
    <% end %>
  </select>
  <div class="gym_session">
  <label for="gym_sessions">Gym_session:</label>
  </div>
  <select name="gym_session_id" id="gym_session">
    <% GymSession.all.each do |gym_session| %>
    <option value="<%= gym_session.id %>"><%= gym_session.name %></option>
    <% end %>
  </select>


  <label for="gym_member"></label>
  <label for="gym_session"></label>


  <input type="submit" value="Add booking"/>
</form>
