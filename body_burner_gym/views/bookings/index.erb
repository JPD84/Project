<h2>All Bookings</h2>
<div class="new_booking">
<a class="new_booking" href="/bookings/new">New Booking</a>
</div>
<hr>
<div class="full-booking-container">
<% @bookings.each do |booking| %>
<div class="booking_details">
  <p>Member name: <%= booking.member.name %></p>
  <p>Gym session name: <%= booking.gym_session.name %></p>
  </div>
  <p><a href="/bookings/<%= booking.id %>">Show bookings</a></p>
  <hr />

<% end %>
</div>
